@model List<RegisteredUser>
@{
    //only test data in here for now, change it later so it works
    ViewData["Title"] = "Profile Page";
}
<div class="table-container">
    <table class="table">
        <thead class="thead-dark">
            <tr>
                <th scope="col">Id</th>
                <th scope="col">Username</th>
                <th scope="col">Email</th>
                <th scope="col">Country</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <th scope="row">@item.Id</th>
                    <td>@item.UserName</td>
                    <td>@item.Email</td>
                    <td>@item.Country</td>
                </tr>

            }
        </tbody>
    </table>
</div>